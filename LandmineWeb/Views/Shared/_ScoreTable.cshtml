@model PagedList.IPagedList<Landmine.Domain.Entities.Score>
<table class="table table-striped table-condensed">
    <thead>
        <tr>
            <td>Nickname</td>
            <td>Level</td>
            <td>Score</td>
        </tr>
    </thead>

    <tbody>
        @foreach (var score in Model)
        {
            <tr>
                <td>@score.Nickname</td>
                <td>@score.Level</td>
                <td>@score.Value</td>
            </tr>
        }
    </tbody>
</table>